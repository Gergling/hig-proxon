{
  "name": "server",
  "version": "1.0.0",
  "main": "src/server.ts",
  "scripts": {
    "build": "tsc",
    "data:backfill": "ts-node src/scripts/backfill.ts",
    "data:extract": "ts-node src/scripts/extract-local.ts",
    "data:transform": "ts-node src/scripts/transform-local.ts",
    "data:s3-test": "dotenv -- ts-node src/scripts/test-s3-storage.ts",
    "generate:notion-sdk": "npx notion-ts-client generate --sdk ./src/notion-sdk",
    "run:production": "node dist/server.js",
    "run:dev": "nodemon --exec ts-node src/server.ts",
    "start:nodemon": "nodemon",
    "start": "npm run run:production",
    "test": "jest --coverage",
    "validate:type-check": "tsc --noEmit",
    "validate:unused": "knip"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "nodemonConfig": {},
  "dependencies": {
    "@aws-sdk/client-s3": "^3.844.0",
    "@aws-sdk/lib-storage": "^3.844.0",
    "@notionhq/client": "^3.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "notion-ts-client": "^0.2.18",
    "temporal-polyfill": "^0.3.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "jest": "^30.0.2",
    "knip": "^5.61.3",
    "nodemon": "^3.1.10",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
